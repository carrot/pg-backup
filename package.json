    "commander": "^2.9.0",
    "configstore": "^2.1.0",
    "when": "^3.7.7"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "coveralls": "^2.11.12",
    "dotenv": "^2.0.0",
    "nyc": "^8.1.0",
    "snazzy": "^4.0.1",
    "standard": "^7.1.2"
  },
  "engines": {
    "node": ">= 6"
  },
  "homepage": "https://github.com/carrot/pg-backup",
